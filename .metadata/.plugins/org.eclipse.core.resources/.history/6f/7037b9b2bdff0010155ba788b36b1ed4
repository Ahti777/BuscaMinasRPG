public interface BuscaMinasVista {
	/**
	 * mostrarMensaje():
	 * @return void
	 */
	void mostrarMensaje(String texto);
	/**
	 * pedirFila():
	 * @return int
	 */
	int pedirFila();
	/**
	 * pedirColumna():
	 * @return int
	 */
	int pedirColumna();
	/**
	 * mostrarTable():
	 * @return void
	 */
	void mostrarTable(Tabla usoTabla);

}
