 */
public interface BuscaMinasVista {
	/**
	 * mostrarMensaje():
	 * @return void
	 */
	void mostrarMensaje(String texto);
	/**
	 * pedirFila():
	 * @return int
	 */
	int pedirFila(int fila);
	/**
	 * pedirColumna():
	 * @return int
	 */
	int pedirColumna(int columna);
	/**
	 * mostrarTable():
	 * @return void
	 */
	void mostrarTable();

}
